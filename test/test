#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x561d94afa1d0 .scope module, "tb_counter_test" "tb_counter_test" 2 3;
 .timescale -9 -12;
v0x561d94b0e0b0_0 .var "clk", 0 0;
v0x561d94b0e150_0 .net "count", 3 0, v0x561d94b0deb0_0;  1 drivers
v0x561d94b0e220_0 .var "rst_n", 0 0;
S_0x561d94afa360 .scope module, "u_counter" "counter" 2 8, 3 1 0, S_0x561d94afa1d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 4 "count";
v0x561d94ac0470_0 .net "clk", 0 0, v0x561d94b0e0b0_0;  1 drivers
v0x561d94b0deb0_0 .var "count", 3 0;
v0x561d94b0df90_0 .net "rst_n", 0 0, v0x561d94b0e220_0;  1 drivers
E_0x561d94ac15f0/0 .event negedge, v0x561d94b0df90_0;
E_0x561d94ac15f0/1 .event posedge, v0x561d94ac0470_0;
E_0x561d94ac15f0 .event/or E_0x561d94ac15f0/0, E_0x561d94ac15f0/1;
    .scope S_0x561d94afa360;
T_0 ;
    %wait E_0x561d94ac15f0;
    %load/vec4 v0x561d94b0df90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x561d94b0deb0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x561d94b0deb0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x561d94b0deb0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x561d94afa1d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561d94b0e0b0_0, 0, 1;
T_1.0 ;
    %delay 10000, 0;
    %load/vec4 v0x561d94b0e0b0_0;
    %inv;
    %store/vec4 v0x561d94b0e0b0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x561d94afa1d0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561d94b0e220_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561d94b0e220_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561d94b0e220_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561d94b0e220_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561d94b0e220_0, 0, 1;
    %delay 200000, 0;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x561d94afa1d0;
T_3 ;
    %vpi_call 2 40 "$monitor", $time, " ns -- rst_n=%b, count=%d", v0x561d94b0e220_0, v0x561d94b0e150_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x561d94afa1d0;
T_4 ;
    %vpi_call 2 44 "$dumpfile", "tb_counter_test.vcd" {0 0 0};
    %vpi_call 2 45 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x561d94afa1d0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_counter_test.v";
    "counter_test.v";
